
  <form action="/register" method="POST">
    <div class="form-group mb-2">
      <label for="username">Enter a username:</label>
      <input class="form-control" type="email" name="email" style="width: 300px; margin: 1em">
      <input class="form-control" type="password" name="password" style="width: 300px; margin: 1em">
      <button type="submit" class="btn btn-primary">Submit</button>
    </div>
  </form>
  